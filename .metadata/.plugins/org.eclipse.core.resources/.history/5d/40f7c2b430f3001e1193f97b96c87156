package final1;

import java.util.HashMap;
import java.util.Map;


public class Board {
	Map<Integer,Post> board = new HashMap<>();
	
	//생성자
	public Board() {
		int id = 0;
		String title = "제목1";
		String content = "!1111";
	}


	//메소드
	//포스트의 id를 이용하여 정보를 받고 post 정보를 추가
	void addPost(Post post) {	
		
		board.put(post.getId(), post);
		post.getId();
	}
	//게시판 삭제

}
