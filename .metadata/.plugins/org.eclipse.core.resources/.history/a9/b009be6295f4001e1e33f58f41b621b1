package whileTest;

import java.util.Scanner;


public class Dori2 {
	void print() {
		Scanner sc = new Scanner(System.in);
		System.out.println("숫자 1,2,3 중 하나를 입력해주세요");
		int num = sc.nextInt();
		if(num == 1) {
			System.out.println("권도희 오늘 같이 카페와줘서 고마워");
		}
		else if(num == 2) {
			System.out.println("이따 같이 맛있는거 먹자");
		}
		else if(num == 3) {
			System.out.println("권도희 말대꾸하지마");
		}else {
			System.out.println("1,2,3중 하나만 입력하라구 바보야!!!!");
		}
	}

}
