package forTest;

public class Ex05_04 {
	public static void main(String[] args) {
		
		for(int i = 0; i < 7; i++) {
			System.out.println(1);
			for(int j = 0; j < 7-i; j++) {
				System.out.print(2);
				for(int k = 0; k < i; k++) {
					System.out.print(3);
			}
			}System.out.println();
		}
	}

}
