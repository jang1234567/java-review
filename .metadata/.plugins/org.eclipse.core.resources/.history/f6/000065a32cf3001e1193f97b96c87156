package final1;

import java.util.HashMap;
import java.util.Map;

public class Board {
	Map<String,String> board = new HashMap<>();
	
	//생성자
	public Board() {
		int id = 0;
		String title = null;
		String content = null;
	}


	//메소드
	//포스트의 id를 이용하여 정보를 받고 post 정보를 추가
	void addPost(Post post) {
		board.put(post.getTitle(), post.getContent());	
	}
	//게시판 삭제

}
