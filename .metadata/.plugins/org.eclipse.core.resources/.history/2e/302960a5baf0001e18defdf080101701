package map;

import java.util.HashMap;

public class HasnMapTest1 {
	public static void main(String[] args) {
		
		HashMap<String,Integer> map = new HashMap<>();
		System.out.println(map);
		
		//추가 put(key,value)
		map.put("A", 1);
		map.put("B", 2);
		map.put("D", 3);
		map.put("C", 4);
		System.out.println(map);
	}

}
