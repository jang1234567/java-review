package construtorTest;

class A { //A클래스 시작
	//메소드 매개변수x, 리턴값x
	void method() {
		System.out.println(this);
	}
}//A클래스 종료

public class ClassTest {
	public static void main(String[] args) {
		A a = new A();
		System.out.println(a);
		a.method();
		A a1 = new A();
		System.out.println(a1);
		a1.method();
	}
}










