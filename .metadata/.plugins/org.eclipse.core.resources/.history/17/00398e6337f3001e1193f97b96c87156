package final1;


	class Post {
		//필드
	    private int id;
	    private String title;
	    private String content;

	    //생성자
	    public Post(int id, String title, String content) {
	        this.id = id;
	        this.title = title;
	        this.content = content;
	    }
	    //getter/setter메소드

	    public int getId() {
	        return id;
	    }

	    public String getTitle() {
	        return title;
	    }

	    public String getContent() {
	        return content;
	    }
	    
	    public void setId(int id) {
	    	this.id = id++;
	    }
	}
