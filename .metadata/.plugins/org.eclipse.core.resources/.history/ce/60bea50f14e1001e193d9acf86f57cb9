package test;

import java.util.Scanner;

public class Ex04 {
	public static void main(String[] args) {
		
		//두개의 정수형 변수 선언, 문자열 변수 1개 선언
		//입력클래스 호출,import
		//입력받을문자 출력
		//sc.nextInt로 변수에 입력받은 값 저장
		//입력받을문자 출력
		//sc.nextInt로 변수에 입력받은 값 저장
		//두수가 같은지 삼항연산자 사용
		
		int num1 = 0, num2 = 0;
		String result = "";
		Scanner sc = new Scanner(System.in);
		System.out.println("정수 1 : ");
		num1 = sc.nextInt();
		System.out.println("정수 2 : ");
		num2 = sc.nextInt();
		//result = num1 == num2 ? "같다" : "다르다";
		//System.out.println(result);
		if(num1 == num2) {
			System.out.println("같다");
		}else {
			System.out.println("다르다");
		}
		
		
	}

}
